        "properties": {
          "id": "zh/content/tutorial-authoring-to-publish/index#012",
          "text": "```bash\ncodex run publish --message \"content: 新增 <标题>\"\n```",
          "order": 12,
          "doc_id": "zh/content/tutorial-authoring-to-publish/index"
        },
        "type": "Chunk",
        "name": null
      },
      {
